type Product{
    id:String!
    name:String
    categoria:String
    price:Float
    stock:Int
}

type inputProduct{
    name:String
    categoria:String
    price:Float
    stock:Int
}

type Query{
    getAllProduct:[Product]
    productByName(name:String!):[Product]
}

type Mutation {
    createProduct(
        name:String,
        categoria:String,
        price:Float,
        stock:Int)
    :Product
}