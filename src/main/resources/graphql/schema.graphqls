type Product{
    id:String!
    name:String
    category:String
    price:Float
    stock:Int
}

type Query{
    getAllProduct:[Product]
    productByName(name:String!):[Product]
}

type Mutation {
    createProduct(Product:ProductDTO!):Product
    deleteProduct(id:String):String
}

input ProductDTO{
    name:String
    category:String
    price:Float
    stock:Int
}